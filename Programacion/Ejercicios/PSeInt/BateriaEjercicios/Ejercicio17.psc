Proceso Calificaciones
	Definir partePractica Como Real;
	Definir parteProblemas Como Real;
	Definir parteTeorica Como Real;
	
	Escribir "Introducir calificación Practica";
	Leer partePractica;
	Escribir "Introducir calificación Problemas";
	Leer parteProblemas;
	Escribir "Introducir calificación Teorica";
	Leer parteTeorica;
	
	Escribir "Nota final";
	Escribir partePractica * 0.1 + parteProblemas * 0.5 + parteTeorica * 0.4; 
	
	
	
FinProceso
